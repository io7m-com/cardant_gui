<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="600.0" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.io7m.cardant_gui.ui.internal.CAGLocationSelectView">
   <children>
      <fx:include fx:id="locationTree" source="locationTree.fxml" />
      <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0">
         <children>
            <Button cancelButton="true" maxHeight="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#onCancelSelected" prefWidth="128.0" text="%cardant.cancel" />
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="select" defaultButton="true" disable="true" maxHeight="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#onSelectSelected" prefWidth="128.0" text="%cardant.select" />
         </children>
         <VBox.margin>
            <Insets top="8.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
