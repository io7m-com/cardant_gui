<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.io7m.cardant_gui.ui.internal.CAGFileSelectView">
   <children>
      <HBox styleClass="borderedOutset" VBox.vgrow="ALWAYS">
         <children>
            <fx:include fx:id="fileSearch" source="fileSearch.fxml" HBox.hgrow="NEVER" />
            <fx:include fx:id="fileList" source="fileList.fxml" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0">
         <VBox.margin>
            <Insets top="8.0" />
         </VBox.margin>
         <children>
            <Button cancelButton="true" maxHeight="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#onCancelSelected" prefHeight="32.0" prefWidth="128.0" text="%cardant.cancel" />
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="select" defaultButton="true" disable="true" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#onSelectSelected" prefHeight="32.0" prefWidth="128.0" text="%cardant.select" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
